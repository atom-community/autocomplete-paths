{
  "name": "autocomplete-paths",
  "main": "./dist/autocomplete-paths.js",
  "version": "2.15.2",
  "description": "Adds path autocompletion to autocomplete+",
  "repository": "https://github.com/atom-community/autocomplete-paths",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "test.format": "prettier . --check",
    "lint": "eslint . --fix",
    "test": "atom --test ./spec",
    "test.lint": "eslint .",
    "clean": "shx rm -rf dist .parcel-cache",
    "dev": "cross-env NODE_ENV=development parcel watch --target main lib/autocomplete-paths.js",
    "build": "cross-env NODE_ENV=production parcel build --target main lib/autocomplete-paths.js",
    "build-commit": "build-commit -o dist",
    "prepare": "npm run build"
  },
  "prettier": "prettier-config-atomic",
  "targets": {
    "main": {
      "context": "electron-renderer",
      "engines": {
        "electron": ">=5.x"
      },
      "includeNodeModules": {
        "atom": false,
        "zadeh": false,
        "fsevents": false
      },
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  },
  "dependencies": {
    "chokidar": "^3.5.1",
    "fast-glob": "^3.2.5",
    "globify-gitignore": "^0.2.0",
    "minimatch": "^3.0.4",
    "slash": "^3.0.0",
    "underscore": "^1.12.0",
    "zadeh": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@types/atom": "^1.40.11",
    "@types/is-valid-path": "^0.1.0",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16.3.3",
    "@types/underscore": "^1.11.3",
    "babel-preset-atomic": "^4.2.0",
    "build-commit": "0.1.4",
    "cross-env": "7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-atomic": "^1.16.2",
    "parcel": "2.0.0-nightly.476",
    "prettier-config-atomic": "^2.0.5",
    "shx": "0.3.3"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
